{
  "manifest_version": 2,
    "name": "Bookmarks Dashboard",
    "version": "1.0",
  
    "description": "See, delete and move all your bookmarks in one place.",
  
    "icons": {
        "16": "icons/bookmark-5-16.png",
        "24": "icons/bookmark-5-24.png",
        "32": "icons/bookmark-5-32.png",
        "48": "icons/bookmark-5-48.png",
        "128": "icons/bookmark-5-128.png"
    },
  "permissions": ["bookmarks"],
  "background": {
    "scripts": ["openapp.js"]
  },

  "browser_action": {
    "default_icon": "icons/bookmark-5-32.png"
  }
}